<!DOCTYPE html>
<html>
<head>
  <title>FlatironBlogApp</title>
  <%= include_gon(:init => true) %>
  <%= Gon::Base.render_data({}) %>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= analytics_init if Rails.env.production? %>
  <%= favicon_link_tag 'flatiron_school_slashes_transparent_background.png' %>
</head>
<body>

<%= render 'layouts/navbar' %>

<div class="container">
  <div id="messages">
    <%= flash_messages %>
  </div>
  <%= yield %>
</div>

</body>
</html>
