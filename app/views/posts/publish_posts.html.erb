<h1>Choose a post to publish!</h1>

<ul>
<% @posts.all.each do |post| %>
  <li><%= post.title %>: written by <%= post.user.full_name_or_github_name %> on <%= post.published_date.strftime("%m/%d/%Y") %> <%= link_to 'publish this post', publish_post_path(wpc: @wpc, post_id: post.id) %></li>
<% end %>
</ul>
